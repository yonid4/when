# Feature Flags
# REACT_APP_ENABLE_CALENDAR_SYNC=true
# REACT_APP_ENABLE_NOTIFICATIONS=true
# REACT_APP_ENABLE_DARK_MODE=true

# Custom Styling
# REACT_APP_PRIMARY_COLOR=#3b82f6
